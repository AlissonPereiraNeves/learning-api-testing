{
    "name": "learning-api-testing",
    "version": "1.0.0",
    "description": "Learning API Testing with Chai, Mocha and SuperTest",
    "author": "Alisson Pereira",
    "dependencies": {
        "body-parser": "^1.17.1",
        "chai": "^4.1.0",
        "mocha": "^5.2.0",
        "mocha-jenkins-reporter": "^0.3.7",
        "supertest": "^3.2.0"
    },
    "scripts": {
        "prestart": "npm install;",
        "start": "JUNIT_REPORT_PATH=test-result/result.xml JUNIT_REPORT_STACK=1 mocha --timeout 25000 --colors --reporter mocha-jenkins-reporter || true"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:alissonpereiraneves/learning-api-testing.git"
    }
}
